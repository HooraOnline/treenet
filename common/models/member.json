{
  "name": "Member",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "permissions": {
      "type": "array",
      "default": [
        "register",
        "me"
      ]
    },
    "roleList": {
      "type": [
        "array"
      ],
      "default": [
        "normal_member"
      ]
    },
    "posts": {
      "type": "array"
    },
    "invitationCode": {
      "type": "string"
    },
    "tempRegentCode": {
      "type": "string"
    },
    "regentCode": {
      "type": "string"
    },
    "regentId": {
      "type": "string"
    },
    "mobile": {
      "type": "string"
    },
    "mobileVerified": {
      "type": "boolean"
    },
    "fullName": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "birthDate": {
      "type": "date"
    },
    "location": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "subsets": {
      "type": "hasMany",
      "model": "Member",
      "foreignKey": "regentId"
    },
    "followeds": {
      "type": "hasMany",
      "model": "Follower",
      "foreignKey": "followerId"
    },
    "followers": {
      "type": "hasMany",
      "model": "Follower",
      "foreignKey": "followedId"
    },
    "posts": {
      "type": "hasMany",
      "model": "Post",
      "foreignKey": "memberId"
    },
    "likes": {
      "type": "hasMany",
      "model": "Like",
      "foreignKey": "memberId"
    },
    "seens": {
      "type": "hasMany",
      "model": "Seen",
      "foreignKey": "memberId"
    },
    "comments": {
      "type": "hasMany",
      "model": "Comment",
      "foreignKey": "memberId"
    }
  },
  "acls": [],
  "methods": {}
}
