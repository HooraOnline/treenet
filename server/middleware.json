{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {},
    "helmet#xssFilter": {"enabled": true},
    "helmet#frameguard": {
      "params": {
        "action": "deny"
      }
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 0,
        "includeSubDomains": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {},
    "helmet#noCache": {
      "enabled": false
    },

    "cors": {
      "params": {
        "origin": ["https://treenetgram.com","https://vercel.app","https://treenetgram.vercel.app","https://uniqu.ir","https://uniqu.vercel.app","http://localhost:3300","http://192.168.100.2:3300"],
        "credentials": false,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {
    "body-parser#json": {},
    "body-parser#urlencoded": {"params": { "extended": true }}
  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../client"
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
        "debug": false,
        "log": true
      }
    }
  }
}


